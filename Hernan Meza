## descartar un elemento de la lista
print("edades de las personas que fueron a vacunarse")
vEdades = [2, 7, 58, 7, 45, 26, 10, 8, 56, 57, 97, 19, 11, 53, 3, 99, 62, 78, 29, 9, 37, 42, 56, 86, 28, 86, 95, 26, 49, 67, 21, 815, 67, 10, 58, 512, 24, 92, 89, 67, 53, 10, 9, 83, 1, 44, 10, 77, 98, 73, 57]
Edades = 10



for edad in vEdades:
    if edad == 10:
        vEdades.remove(10)


for edad in vEdades:
    print("edad", edad, "a√±os")



total = len(vEdades)
print("total", total+1)
print(vEdades)

